
            {
              "manifest_version": 3,
              "name": "Proxy Extension",
              "version": "1.0",
              "permissions": [
                "proxy",
                "storage",
                "webRequest",
                "webRequestAuthProvider"
              ],
              "host_permissions": [
                "<all_urls>"
              ],
              "background": {
                "service_worker": "background.js"
              },
              "action": {
                "default_title": "Proxy Extension"
              }
            }
            